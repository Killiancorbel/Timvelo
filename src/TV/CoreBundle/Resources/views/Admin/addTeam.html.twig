{% extends "::layout.html.twig" %}

{% block body_left %}
	<div class="search">
		<input type="text" name="search" class="round" />
	</div>
	<div class="results">
		{% for team in teams %}
			<div class="element">
				<h2><a href="{{ path('tv_core_edit_team', {'id': team.id}) }}">{{ team.name }}</a></h2>
				<p>{{ team.fullname }}<br />
				   {{ team.nickname }}</p>
			</div>
		{% endfor %}
	</div>
{% endblock %}

{% block body_right %}
	{{ form(form) }}
{% endblock %}